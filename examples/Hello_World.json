{
  "name": "Hello World",
  "method": "GET",
  "path": "/hello/{name}",
  "statusCode": 200,
  "simulateStreaming": false,
  "headers": [{
    "name": "Content-Type",
    "value": "application/json"
  }],
  "response": {
    "greetingText": "Hello $name!",
    "inValue_$name": "Some random value",
    "inArrayObjNode": [
      {
        "Hello": "$name"
      }
    ],
    "inStrArray": ["Hello", "$name"]
  }
}